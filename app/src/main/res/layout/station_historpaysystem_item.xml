    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:gravity="center_vertical"
        android:background="#ffffff">

        <TextView
            android:id="@+id/tv_date"

            android:layout_width="match_parent"

            android:layout_height="wrap_content"
            android:text="2017-05-12  12:30:03"
            />

        <TextView
            android:id="@+id/tv_outsName"
            android:textSize="16sp"
            android:layout_width="match_parent"
            android:layout_marginTop="@dimen/x30"
            android:layout_height="wrap_content"
            android:text="xx代售点"
            />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:gravity="center_vertical"
            android:layout_marginTop="@dimen/x20"
            >

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="应扣"
                    android:textSize="16sp"
                    android:textColor="@color/grayss"
                    />
                <TextView
                    android:id="@+id/tv_shouldAmount"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0.00"
                    android:textSize="16sp"
                    android:textColor="@color/blues"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/grayss"
                    android:textSize="16sp"
                    android:text="元"
                    />

            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="right"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="扣款状态："
                    android:textSize="16sp"
                    />

                <TextView
                    android:id="@+id/tv_withholdStatus"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="成功"
                    android:textSize="16sp"
                    android:textColor="@color/grayss"
                    />


            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:gravity="center_vertical"
            android:layout_marginTop="@dimen/x20"
            >
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="已扣"
                    android:textSize="16sp"
                    android:textColor="@color/grayss"
                    />
                <TextView
                    android:id="@+id/tv_alreadyAmount"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0.00"
                    android:textSize="16sp"
                    android:textColor="@color/blues"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/grayss"
                    android:textSize="16sp"
                    android:text="元"
                    />

            </LinearLayout>



            <TextView
                android:gravity="right"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"

                android:text="汇缴窗口号："
                />

            <TextView
                android:id="@+id/tv_payWinNumber"
                android:gravity="right"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="1234"
                android:textColor="@color/blues"
                />
        </LinearLayout>



    </LinearLayout>

</LinearLayout>
